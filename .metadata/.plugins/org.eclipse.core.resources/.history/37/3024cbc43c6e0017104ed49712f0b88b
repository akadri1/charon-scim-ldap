package org.wso2.charon3.utils.ldapmanager;

/**
 * @author AkshathaKadri
 *
 */
public enum LdapScimAddressMap {

	streetAddress("street"),locality("l"), region("st"), postalCode("postalCode"), country("destinationIndicator");
	
	
	//BAD JUJU
	/*	 manager("manager"),  */
	
	private final String value;
	private boolean isSet;
	LdapScimAddressMap(String value) {
		this.value = value;
	}

	public String getValue() {
		return value;
	}
	public boolean isSet() {
		return isSet;
	}
	public void setSet(boolean isSet) {
		this.isSet = isSet;
	}
}
