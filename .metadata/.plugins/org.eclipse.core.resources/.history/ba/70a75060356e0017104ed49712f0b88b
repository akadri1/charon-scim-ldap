package org.wso2.charon3.utils.ldapmanager;

/**
 * @author AkshathaKadri
 *
 */
public enum LdapScimAddressMap {
	
	externalId("x500UniqueIdentifier"), id("uid"), userName("cn"), familyName("sn"), password("userPassword"), profileUrl("labeledUri"),
	displayName("displayName"), nickName("givenName"),formatted("fullName"), givenName("givenName"), middleName("Initials"), active("loginDisabled"),
	userType("employeeType"), title("title"), employeeNumber("employeeNumber"), costCenter("costCenter"),
	resourceType("carLicense"), created("street") ,version("revision"),lastModified("company"),
	honorificPrefix("Initials"), preferredLanguage("preferredLanguage"), locale("physicalDeliveryOfficeName"),
	organization("o"), division("ou"), department("organizationalUnitName"),
	timezone("Timezone"),ims("instantMessagingID"), x509Certificates("userCertificate"), photos("localityName"), location("audio"), 
	honorificSuffix("description") ;
	
	//BAD JUJU
	/*	 manager("manager"),  */
	
	private final String value;
	private boolean isSet;
	LdapScimAddressMap(String value) {
		this.value = value;
	}

	public String getValue() {
		return value;
	}
	public boolean isSet() {
		return isSet;
	}
	public void setSet(boolean isSet) {
		this.isSet = isSet;
	}
}
