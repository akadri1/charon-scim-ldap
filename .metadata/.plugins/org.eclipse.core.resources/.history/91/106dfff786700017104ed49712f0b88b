package org.wso2.charon3.utils.ldapmanager;

/**
 * @author AkshathaKadri
 *
 */
public enum LdapScimAttrMap {
	//id("uid"), externalId("x500UniqueIdentifier"), userName("mail"), password("userPassword"),givenName("cn"),familyName("sn"),nickName("givenName");
	externalId(LdapAttributeConstants.x500UniqueIdentifier), 
	id(LdapAttributeConstants.uid), 
	userName(LdapAttributeConstants.cn), 
	familyName(LdapAttributeConstants.sn), 
	password(LdapAttributeConstants.userPassword), 
	profileUrl(LdapAttributeConstants.labeledUri),
	userType(LdapAttributeConstants.employeeType), 
	title(LdapAttributeConstants.title), 
	employeeNumber(LdapAttributeConstants.employeeNumber), 
	costCenter(LdapAttributeConstants.costCenter), 
	active(LdapAttributeConstants.loginDisabled),
	
	displayName(LdapAttributeConstants.displayName), 
	nickName(LdapAttributeConstants.givenName),
	formatted(LdapAttributeConstants.fullName), 
	givenName(LdapAttributeConstants.givenName), 
	middleName(LdapAttributeConstants.Initials),
	honorificPrefix(LdapAttributeConstants.Initials), 
	honorificSuffix(LdapAttributeConstants.description),
	
	phoneNumbers_work(LdapAttributeConstants.telephoneNumber), 
	phoneNumbers_mobile(LdapAttributeConstants.homePhone),
	emails_work(LdapAttributeConstants.mail), 
	emails_home(LdapAttributeConstants.x121Address),
	photos_photo(LdapAttributeConstants.localityName), 
	photos_thumbnail(LdapAttributeConstants.description), 
	
	resourceType(LdapAttributeConstants.carLicense), 
	created(LdapAttributeConstants.businessCategory) ,
	version(LdapAttributeConstants.revision),
	lastModified(LdapAttributeConstants.company),
	
	preferredLanguage(LdapAttributeConstants.preferredLanguage), 
	locale(LdapAttributeConstants.physicalDeliveryOfficeName),
	organization(LdapAttributeConstants.o), 
	division(LdapAttributeConstants.ou), 
	department(LdapAttributeConstants.organizationalUnitName),
	timezone(LdapAttributeConstants.Timezone),
	ims(LdapAttributeConstants.instantMessagingID), 
	x509Certificates(LdapAttributeConstants.userCertificate), 
	location(LdapAttributeConstants.audio), 
	
	streetAddress(LdapAttributeConstants.street),
	locality(LdapAttributeConstants.l), 
	region(LdapAttributeConstants.st), 
	postalCode(LdapAttributeConstants.postalCode), 
	country(LdapAttributeConstants.destinationIndicator), 
	groups(LdapAttributeConstants.groupMembership);
	
	//BAD JUJU
	/*
	 
	 manager("manager"),  */
	
	private final String value;
	private boolean isSet;
	LdapScimAttrMap(String value) {
		this.value = value;
	}

	public String getValue() {
		return value;
	}
	public boolean isSet() {
		return isSet;
	}
	public void setSet(boolean isSet) {
		this.isSet = isSet;
	}
}
